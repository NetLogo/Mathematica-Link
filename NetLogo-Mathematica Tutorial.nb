   3.3961359595606003`*^9, 3.396135964258638*^9}}],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"B", "[", "v_", "]"}], ":=", 
   RowBox[{"v", " ", 
    SuperscriptBox["E", 
     FractionBox[
      RowBox[{
       RowBox[{"-", "m"}], " ", 
       SuperscriptBox["v", "2"]}], 
      RowBox[{"2", "k", " ", "T"}]]]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"normalizer", "=", " ", 
